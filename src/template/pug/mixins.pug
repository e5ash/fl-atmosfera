mixin psettingSelect(list)
	.psetting__select.select.--sm.--b-gray.--p20
		.select__area
			.select__title
			.select__icon
				<svg width="10" height="7" viewBox="0 0 10 7" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M1 1.01733L5 5.23066L9 1.01733" stroke="#3F3F3F" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg>
		.select__drop
		select.select__parse
			- i = 0
			each item in list
				option(value=item selected=i==0 ? "selected" : null)= item
				- i++ 
mixin reviewsItem(title, text)
	.reviews__item
		.reviews__title= title
		.reviews__text= text

mixin imgs(imgs)
	.product__imgs.imgs
		.imgs__main.swiper
			.imgs__list.swiper-wrapper
				each img in imgs
					.imgs__item.img.swiper-slide(class=imgsVideos ? '--video' : null data-fancybox="" data-src=imgsVideos ? 'https://www.youtube.com/watch?v=kJQP7kiw5Fk' : img data-group="product")
						img(src=img alt="")
		.imgs__nav.swiper
			.imgs__nav-list.swiper-wrapper
				each img in imgs
					.imgs__nav-item.img.swiper-slide
						img(src=img alt="")


mixin ctrls(cls)
	div(class=cls class="ctrls")
		.ctrls__btns
			.ctrls__btn
				img(src="uploads/cart-xs.png" alt="")
			if (!attributes.puzzlesHidden)
				.ctrls__btn
					img(src="uploads/puzzle-xs.png" alt="")
			.ctrls__btn
				img(src="uploads/heart-xs.png" alt="")
		.ctrls__likes
			.ctrls__like.--up
				.ctrls__like-value 10
				.ctrls__like-icon
					img(src="uploads/thumbs-up-white.png" alt="")
			.ctrls__like.--down
				.ctrls__like-value 3
				.ctrls__like-icon
					img(src="uploads/thumbs-down-white.png" alt="")


