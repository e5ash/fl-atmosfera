include mixins/style
include mixins/script
include mixins/breadcrumbs

mixin fotostokItem(name, img, disabled)
	a.fotostok__item(href="" class=disabled ? '--disabled' : null)
		.fotostok__img.img
			img(src='uploads/hist/' + img + '.jpg' alt="")
		.fotostok__name #[span= name]
		.fotostok__icon
			img(src='uploads/sports/sport-'+img + '.png' alt="")

mixin sectionItem(name, img, dates, desc)
	a.section__item(href="")
		.section__dates
			each date in dates
				.section__date= date
		.section__img
			img(src=img alt="")
		.section__name #[span= name]
		.section__desc= desc

mixin catsItem(name, img)
	a.cats__item(href="")
		.cats__img
			img(src="uploads/" + img alt="")
		.cats__name #[span= name]


