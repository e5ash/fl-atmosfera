mixin caevItem(title, img, sport, cls = 'swiper-slide')
	.caev__item(class=cls)
		.caev__item-inner
			.caev__img-wrap
				.caev__date #[span 03.10.2021]
				.caev__img.img
					img(src="uploads/events/" + img alt="")
				.caev__sport
					img(src="uploads/sports/" + sport alt="")
			.caev__content
				a.caev__title(href="")= title
				a.caev__btn.btn.--md.--bg-orange(href="") Предзаказ

mixin calDay(count, events, disabled)
	td
		.cal__day(class=disabled ? '--disabled' : null)
			.cal__count=count
			if events
				.cal__events
					each event in events
						.cal__event
							.cal__event-img
								img(src="uploads/sports/sport-"+event+".png" alt="")
							.cal__event-data
								p 13.00 Baltic Cup 2021
								p 14.00 One Day Junior Cup 2021
								p 14.30 SPARTA CUP 2021
								p 15.30 Baltic Cup 2021
								p 16.00 One Day Junior Cup 2021
								p 16.00 SPARTA CUP 2021
								p 16.30 Baltic Cup 2021
								p 16.30 One Day Junior Cup 2021
								p 17.00 SPARTA CUP 2021
								p 17.30 Baltic Cup 2021


