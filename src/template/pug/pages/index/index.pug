				.calendar__inner.swiper
					.calendar__list.swiper-wrapper
						mixin caledarMoth(name, current)
							.calendar__item.swiper-slide
								.calendar__month= name 
								.calendar__days
									.calendar__day #[var 1] #[span Пн]
									.calendar__day #[var 2] #[span Вт]
									.calendar__day #[var 3] #[span Ср]
									.calendar__day #[var 4] #[span Чт]
									.calendar__day #[var 5] #[span Пт]
									.calendar__day.--red #[var 6] #[span Сб]
									.calendar__day.--red #[var 7] #[span Вс]
									.calendar__day #[var 8] #[span Пн]
									.calendar__day #[var 9] #[span Вт]
									.calendar__day #[var 10] #[span Ср]
									.calendar__day #[var 11] #[span Чт]
									.calendar__day #[var 12] #[span Пт]
									.calendar__day.--red(class=current ? '--current' : null) #[var 13] #[span Сб]
									.calendar__day.--red #[var 14] #[span Вс]
									.calendar__day #[var 15] #[span Пн]
									.calendar__day #[var 16] #[span Вт]
									.calendar__day #[var 17] #[span Ср]
									.calendar__day #[var 18] #[span Чт]
									.calendar__day #[var 19] #[span Пт]
									.calendar__day.--red #[var 20] #[span Сб]
									.calendar__day.--red #[var 21] #[span Вс]
									.calendar__day #[var 22] #[span Пн]
									.calendar__day #[var 23] #[span Вт]
									.calendar__day #[var 24] #[span Ср]
									.calendar__day #[var 25] #[span Чт]
									.calendar__day #[var 26] #[span Пт]
									.calendar__day.--red #[var 27] #[span Сб]
									.calendar__day.--red #[var 28] #[span Вс]
									.calendar__day #[var 29] #[span Пн]
									.calendar__day #[var 30] #[span Вт]
						+caledarMoth('Ноябрь')
						+caledarMoth('Декабрь')
						+caledarMoth('Январь')

