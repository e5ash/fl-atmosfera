extends ../../templates/default
block vars
	-
		title = "Видеотека"
		breadcrumbs = [
			['', '.html'],
			[title]
		]
block content
	include ../../../blocks/intro/intro
	include ../../../blocks/sports/sports
	.page
		.case
			// +breadcrumbs
			.page__wrap.row
				.page__aside
					include ../../../blocks/navus/navus
				.page__section
					.page__head
						.page__icon
							img(src="uploads/" alt="")
						.page__title.h3= title
					.page__body
						.fotostok
							.fotostok__list.row
								+fotostokItem('Гандбол', '', '')
								+fotostokItem('Футбол', '', '', true)
								+fotostokItem('Флорбол', '', '', true)
								+fotostokItem('Водные виды спорта', '', '', true)
								+fotostokItem('Фигурное катание', '', '', true)
								+fotostokItem('Пляжный волейбол', '', '', true)
								+fotostokItem('Танцы', '', '', true)
								+fotostokItem('Фехтование', '', '')
								+fotostokItem('Большой тенис', '', '')
								+fotostokItem('Хокей', '', '')
								+fotostokItem('Футзал', '', '')
								+fotostokItem('Гимнастика', '', '')
								+fotostokItem('Баскетбол', '', '')
								+fotostokItem('Боевые единоборства', '', '')
								+fotostokItem('Бадминтон', '', '')
								+fotostokItem('Спорт для семьи', '', '')
								+fotostokItem('Детское творчество', '', '', true)